<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 



<hibernate-mapping package="com.lti.hibernateegs.HibernateProject">
   <class name = "Employeees" table = "employees">
      
     
      <id name = "id"  column = "id">
         <generator class="sequence">
         <param name ="sequence">seq</param></generator>
      </id>
      
      <property name = "empName" column = "empname" />
      <property name = "branch"  column = "branch"/> 
     
      
   </class>
</hibernate-mapping>